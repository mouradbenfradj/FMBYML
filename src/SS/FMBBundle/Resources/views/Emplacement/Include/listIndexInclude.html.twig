{% if page == "suivit" %}
    {% for id,emplacement in flotteur %}
        {% if not loop.first %}
            <div class="col-xs-1 col-sm-1 col-md-1">
                {% if emplacement.stockscorde == null and emplacement.stockslanterne == null %}
                    <font color="red"> {{ emplacement.place }}</font>
                {% elseif emplacement.stockscorde != null %}
                    {% include "SSFMBBundle:Include:suivitEtatCordeInclude.html.twig" %}
                {% elseif emplacement.stockslanterne != null %}
                    {% include "SSFMBBundle:Include:suivitEtatLanterneInclude.html.twig" %}
                {% endif %}
            </div>
        {% endif %}
    {% endfor %}
{% elseif page == "MAEL" %}
    {% for emp,emplacement in flotteur %}
        {% if not loop.first %}
            <div class="col-xs-1 col-sm-1 col-md-1">
                {% if emplacement.stockscorde == null and emplacement.stockslanterne == null %}
                    {% set verifexsistance =  verifexsistance + 1 %}
                    <font color="red"> {{ emplacement.place }}</font>
                    <input type="checkbox" name="placelanterne[]" value="{{ emp }}"/>
                {% elseif emplacement.stockscorde != null %}
                    {% include "SSFMBBundle:Include:suivitEtatCordeInclude.html.twig" %}
                {% elseif emplacement.stockslanterne != null %}
                    {% include "SSFMBBundle:Include:suivitEtatLanterneInclude.html.twig" %}
                {% endif %}
            </div>
        {% endif %}
    {% endfor %}
    <br>
    {% if verifexsistance != 0 %}
        <div class="cochage">
            <div class="col-xs-11 col-sm-11 col-md-11">
                <input type="button" value="Tout cocher"
                       onClick="fillCheckbox('{{ filiere.nomFiliere }}{{ segment.nomSegment }}{{ flotteur.nomFlotteur }}','1');">&nbsp;&nbsp;
                <input type="button" value="Tout décocher"
                       onClick="fillCheckbox('{{ filiere.nomFiliere }}{{ segment.nomSegment }}{{ flotteur.nomFlotteur }}','0');">
            </div>
        </div>
    {% endif %}
