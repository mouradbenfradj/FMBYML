{% set somme = 0 %}
{% set i=0 %}

    {% for segment in filiere %}
        {% for flotteur in segment %}
            {% for emplacement in flotteur %}
                {% set somme = somme +emplacement.qte %}
                {% set i=i+1 %}
            {% endfor %}
        {% endfor %}
    {% endfor %}
<p>{{ somme|number_format(0, ',', '.') }} u / {{ i }} lanterne</p>
<p>{{ (somme /12)|number_format(2, ',', '.') }} dz</p>