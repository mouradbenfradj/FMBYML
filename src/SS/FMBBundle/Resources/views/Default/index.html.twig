{% extends "::base.html.twig" %}
{% block title %}{{ parent() }} - FMB{% endblock %}
{% block stylesheet %}
    {% if(entity != null) %}
        <link href="{{ asset('bundles/ssfmb/plugins/nestable/jquery.nestable.css') }}" rel="stylesheet"/>
        {{ parent() }}
        <link href="{{ asset('bundles/ssfmb/plugins/bootstrap-sweetalert/sweet-alert.css') }}" rel="stylesheet"
              type="text/css">
    {% endif %}
{% endblock %}
{% block header %} {{ parent() }}{% endblock %}
{% block body %}
    {{ parent() }}
    {% set page = 'Statistique' %}
    <div class="row">
        <div class="col-lg-3 col-sm-6">
            <div class="widget-panel widget-style-2 bg-white">
                <i class="md md-store-mall-directory text-info"></i>
                <h2 class="m-0 text-dark counter font-600"><span data-plugin="counterup">{{ entity|length }}</span></h2>
                <div class="text-muted m-t-5">Total Parc</div>
            </div>
        </div>
        <div class="col-lg-3 col-sm-6">
            <div class="widget-panel widget-style-2 bg-white">
                <i class="md ion-android-mixer text-primary"></i>
                <h2 class="m-0 text-dark counter font-600"><span data-plugin="counterup">{{ nbrfiliere }}</span></h2>
                <div class="text-muted m-t-5">Total Filière</div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-3 col-sm-6">
            <div class="widget-panel widget-style-2 bg-white">
                <i class="md ti-align-justify text-pink"></i>
                <h2 class="m-0 text-dark counter font-600"><span data-plugin="counterup">
                        {{ nbrcorde }}
                    </span></h2>
                <div class="text-muted m-t-5">Totale Corde</div>
            </div>
        </div>
        <div class="col-lg-3 col-sm-6">
            <div class="widget-panel widget-style-2 bg-white">
                <i class="md ti-align-justify text-pink"></i>
                <h2 class="m-0 text-dark counter font-600"><span data-plugin="counterup">
                        {{ nbrcordeae }}
                    </span></h2>
                <div class="text-muted m-t-5"> Corde en eau</div>
            </div>
        </div>
        <div class="col-lg-3 col-sm-6">
            <div class="widget-panel widget-style-2 bg-white">
                <i class="md ti-align-justify text-pink"></i>
                <h2 class="m-0 text-dark counter font-600"><span data-plugin="counterup">
                        {{ nbrcordev }}
                    </span></h2>
                <div class="text-muted m-t-5">Corde vide</div>
            </div>
        </div>
        <div class="col-lg-3 col-sm-6">
            <div class="widget-panel widget-style-2 bg-white">
                <i class="md ti-align-justify text-pink"></i>
                <h2 class="m-0 text-dark counter font-600"><span data-plugin="counterup">
                        {{ nbrcordep }}
                    </span></h2>
                <div class="text-muted m-t-5">Corde Preparé</div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-3 col-sm-6">
            <div class="widget-panel widget-style-2 bg-white">
                <i class="md typcn typcn-th-menu-outline text-custom"></i>
                <h2 class="m-0 text-dark counter font-600"><span data-plugin="counterup">

                        {{ nbrlanterne }}</span></h2>
                <div class="text-muted m-t-5">Totale Lanterne</div>
            </div>
        </div>
        <div class="col-lg-3 col-sm-6">
            <div class="widget-panel widget-style-2 bg-white">
                <i class="md typcn typcn-th-menu-outline text-custom"></i>
                <h2 class="m-0 text-dark counter font-600"><span data-plugin="counterup">

                        {{ nbrlanterneae }}</span></h2>
                <div class="text-muted m-t-5">Lanterne  en eau</div>
            </div>
        </div>
        <div class="col-lg-3 col-sm-6">
            <div class="widget-panel widget-style-2 bg-white">
                <i class="md typcn typcn-th-menu-outline text-custom"></i>
                <h2 class="m-0 text-dark counter font-600"><span data-plugin="counterup">

                        {{ nbrlanternev }}</span></h2>
                <div class="text-muted m-t-5">Lanterne vide</div>
            </div>
        </div>
        <div class="col-lg-3 col-sm-6">
            <div class="widget-panel widget-style-2 bg-white">
                <i class="md typcn typcn-th-menu-outline text-custom"></i>
                <h2 class="m-0 text-dark counter font-600"><span data-plugin="counterup">

                        {{ nbrlanternep }}</span></h2>
                <div class="text-muted m-t-5">Lanterne Preparé</div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-3 col-sm-6">
            <div class="widget-panel widget-style-2 bg-white">
                <i class="md typcn typcn-social-dribbble-circular text-custom"></i>
                <h2 class="m-0 text-dark counter font-600"><span data-plugin="counterup">
                        {{ nbrpoche }}
                    </span></h2>
                <div class="text-muted m-t-5">Totale Poche</div>
            </div>
        </div>
        <div class="col-lg-3 col-sm-6">
            <div class="widget-panel widget-style-2 bg-white">
                <i class="md typcn typcn-social-dribbble-circular text-custom"></i>
                <h2 class="m-0 text-dark counter font-600"><span data-plugin="counterup">
                        {{ nbrpocheae }}
                    </span></h2>
                <div class="text-muted m-t-5">Poche en eau</div>
            </div>
        </div>
        <div class="col-lg-3 col-sm-6">
            <div class="widget-panel widget-style-2 bg-white">
                <i class="md typcn typcn-social-dribbble-circular text-custom"></i>
                <h2 class="m-0 text-dark counter font-600"><span data-plugin="counterup">
                        {{ nbrpochev }}
                    </span></h2>
                <div class="text-muted m-t-5">Poche vide</div>
            </div>
        </div>
        <div class="col-lg-3 col-sm-6">
            <div class="widget-panel widget-style-2 bg-white">
                <i class="md typcn typcn-social-dribbble-circular text-custom"></i>
                <h2 class="m-0 text-dark counter font-600"><span data-plugin="counterup">
                        {{ nbrpochep }}
                    </span></h2>
                <div class="text-muted m-t-5">Poche Preparé</div>
            </div>
        </div>
    </div>

{% endblock %}
{% block javascripts %}
    {{ parent() }}
    {% if(entity != null) %}
        <!--script for this page only-->
        <script src="{{ asset('bundles/ssfmb/plugins/nestable/jquery.nestable.js') }}"></script>
        <script src="{{ asset('bundles/ssfmb/pages/nestable.js') }}"></script>
    {% endif %}
    <!-- Moment js  -->
    <script src="{{ asset('bundles/ssfmb/plugins/moment/moment.js') }}"></script>

    <!-- Counter Up  -->
    <script src="{{ asset('bundles/ssfmb/plugins/waypoints/lib/jquery.waypoints.js') }}"></script>
    <script src="{{ asset('bundles/ssfmb/plugins/counterup/jquery.counterup.min.js') }}"></script>

    <!-- Sweet Alert  -->
    <script src="{{ asset('bundles/ssfmb/plugins/bootstrap-sweetalert/sweet-alert.min.js') }}"></script>

    <!-- skycons -->
    <script src="{{ asset('bundles/ssfmb/plugins/skyicons/skycons.min.js') }}" type="text/javascript"></script>

    <!-- Peity chart js -->
    <script src="{{ asset('bundles/ssfmb/plugins/peity/jquery.peity.min.js') }}"></script>

    <!-- Widget js -->
    <script src="{{ asset('bundles/ssfmb/pages/jquery.widgets.js') }}"></script>

    <!-- Todojs  -->
    <script src="{{ asset('bundles/ssfmb/pages/jquery.todo.js') }}"></script>

    <!-- chatjs  -->
    <script src="{{ asset('bundles/ssfmb/pages/jquery.chat.js') }}"></script>

    <!-- Knob -->
    <script src="{{ asset('bundles/ssfmb/plugins/jquery-knob/jquery.knob.js') }} "></script>

    <!-- Sparkline chart -->
    <script src="{{ asset('bundles/ssfmb/plugins/jquery-sparkline/jquery.sparkline.min.js') }}"></script>

    <!-- App core js -->
    <script src="{{ asset('bundles/ssfmb/assets/js/jquery.core.js') }}"></script>
    <script src="{{ asset('bundles/ssfmb/assets/js/jquery.app.js') }}"></script>

    <script type="text/javascript">
        jQuery(document).ready(function ($) {
            $(".knob").knob();
        });
    </script>

{% endblock %}