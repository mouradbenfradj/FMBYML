<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="A fully featured admin theme which can be used to build CRM, CMS, etc.">
    <meta name="author" content="Coderthemes">
    <link rel="shortcut icon" href="{{ asset('favicon.ico' ) }}">
    <title>{% block title %}FMB{% endblock %}</title>
    <link href="{{ asset('bundles/ssfmb/assets/css/bootstrap.min.css') }}" rel="stylesheet" type="text/css"/>
    <link href="{{ asset('bundles/ssfmb/assets/css/core.css') }}" rel="stylesheet" type="text/css"/>
    <link href="{{ asset('bundles/ssfmb/assets/css/components.css') }}" rel="stylesheet" type="text/css"/>
    <link href="{{ asset('bundles/ssfmb/assets/css/icons.css') }}" rel="stylesheet" type="text/css"/>
    <link href="{{ asset('bundles/ssfmb/assets/css/pages.css') }}" rel="stylesheet" type="text/css"/>
    <link href="{{ asset('bundles/ssfmb/assets/css/menu.css') }}" rel="stylesheet" type="text/css"/>
    <link href="{{ asset('bundles/ssfmb/assets/css/responsive.css') }}" rel="stylesheet" type="text/css"/>
    <link rel="stylesheet" href="{{ asset('bundles/ssfmb/css/index.css') }}">
    <link rel="stylesheet" href="{{ asset('bundles/ssfmb/css/sidenavi-right.css') }}">
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <script src="{{ asset('bundles/ssfmb/assets/js/modernizr.min.js') }}"></script>
    <link rel="stylesheet" href="{{ asset('css/nprogress.css') }}">
    <link rel="stylesheet" href="{{ asset('bundles/ssfmb/css/baseTwig/base.css') }}">
    {% block stylesheet %}
    {% endblock %}
</head>
<body>
<span class='nprogress-logo fade out'></span>
<header id="topnav">
    {% block header %}
        <div class="topbar-main">
            <div class="container">
                <div class="logo">
                    <a href="{{ path('ssfmb_homepage') }}" class="logo">
                        <span>
                            <i class="md md-album"></i>yster
                            Pr<i class="md md-album"></i>
                        </span>
                    </a>
                </div>
                <div class="menu-extras">
                    <ul class="nav navbar-nav navbar-right pull-right">
                        <li class="dropdown navbar-c-items">
                            <a href="" class="dropdown-toggle waves-effect waves-light profile" data-toggle="dropdown"
                               aria-expanded="true"><img src="{{ asset('bundles/ssfmb/assets/images/avatar-1.jpg') }}"
                                                         alt="user-img"
                                                         class="img-circle"> </a>
                            <ul class="dropdown-menu">
                                <li>
                                    <a href="javascript:void(0)"><i class="ti-user text-custom m-r-10"></i>
                                        {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
                                            {{ 'layout.logged_in_as'|trans({'%username%': app.user.username}, 'FOSUserBundle') }}
                                        {% else %}
                                            <a href="{{ path('fos_user_security_login') }}">{{ 'layout.login'|trans({}, 'FOSUserBundle') }}   </a>
                                        {% endif %}
                                    </a>
                                </li>
                                <li>
                                    <a href="{{ path('sonata_admin_dashboard') }}"><i
                                                class="ti-settings text-custom m-r-10"></i> Administration</a>
                                </li>
                                <li>
                                    <a href="{{ path('sonata_admin_dashboard') }}">
                                        <i class="ti-lock text-custom m-r-10"></i> Administration
                                    </a>
                                </li>
                                <li class="divider"></li>
                                <li>
                                    <a href="{{ path('fos_user_security_logout') }}">
                                        <i class="ti-power-off text-danger m-r-10"></i> Logout
                                    </a>
                                </li>
                            </ul>
                        </li>
                    </ul>
                    <div class="menu-item">
                        <a class="navbar-toggle">
                            <div class="lines">
                                <span></span>
                                <span></span>
                                <span></span>
                            </div>
                        </a>
                        <!-- End mobile menu toggle-->
                    </div>
                </div>
            </div>
        </div>
        <div class="navbar-custom">
            <div class="container">
                <div id="navigation">
                    {% include "@SSFMB/Default/menu.html.twig" %}
                </div>
            </div>
        </div>
    {% endblock %}
</header>
<div class="row"><br></div>
<div class="wrapper">
    <div class="container">
        <div class="">
            {% block body %}
                {% block footer %}
                    <footer class="footer text-right">
                        <div class="container">
                            <div class="row">
                                <div class="col-xs-6">
                                    Â© 2016. All rights reserved. / POWERED BY FMB & SIDRATSOFT
                                </div>
                                <div class="col-xs-6">
                                </div>
                            </div>
                        </div>
                    </footer>
                {% endblock %}
            {% endblock %}
            <div class="row">
                <div class="col-xs-11">
                </div>
                <div class="col-xs-1">
                    <a id="back-to-top" href="#" class="btn btn-primary btn-lg back-to-top" role="button"
                       title="Click to return on the top page" data-toggle="tooltip" data-placement="left"><span
                                class="glyphicon glyphicon-chevron-up"></span></a>
                </div>
            </div>
        </div> <!-- end container -->
    </div>
    <script src="{{ asset('js/nprogress.js') }}"></script>
    <script src="{{ asset('js/sonic.js') }}"></script>

    <!-- end wrapper -->
    <!-- jQuery  -->
    <script src="{{ asset('bundles/ssfmb/assets/js/jquery.min.js') }}"></script>
    <script src="{{ asset('bundles/ssfmb/assets/js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('bundles/ssfmb/assets/js/detect.js') }}"></script>
    <script src="{{ asset('bundles/ssfmb/assets/js/fastclick.js') }}"></script>
    <script src="{{ asset('bundles/ssfmb/assets/js/jquery.slimscroll.js') }}"></script>
    <script src="{{ asset('bundles/ssfmb/assets/js/jquery.blockUI.js') }}"></script>
    <script src="{{ asset('bundles/ssfmb/assets/js/waves.js') }}"></script>
    <script src="{{ asset('bundles/ssfmb/assets/js/wow.min.js') }}"></script>
    <script src="{{ asset('bundles/ssfmb/assets/js/jquery.nicescroll.js') }}"></script>
    <script src="{{ asset('bundles/ssfmb/assets/js/jquery.scrollTo.min.js') }}"></script>


    <!-- App core js -->
    <script src="{{ asset('bundles/ssfmb/assets/js/jquery.core.js') }}"></script>
    <script src="{{ asset('bundles/ssfmb/assets/js/jquery.app.js') }}"></script>
    <script src="{{ asset('bundles/ssfmb/js/SideNavi.js') }}"></script>
    <script src="{{ asset('bundles/ssfmb/js/baseTwig/base.js') }}"></script>
    {% block javascripts %}
    {% endblock %}
</body>
</html>